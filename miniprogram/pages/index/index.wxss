/**index.wxss**/
.container {
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}

.scrollarea {
  flex: 1;
  overflow-y: hidden;
}

.calendar-btn {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 40%;
}

.calendar-icon {
  width: 128rpx;
  height: 128rpx;
  margin-bottom: 10rpx;
}

.calendar-text {
  color: #333;
  font-size: 28rpx;
}
