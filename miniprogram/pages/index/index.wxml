<!--index.wxml-->
<view class="container">
  <view class="calendar-btn" bindtap="onCalendarTap">
    <view class="btn-content">
      <image class="calendar-icon" src="/images/calendar.png" mode="aspectFit"></image>
      <view class="text-container">
        <text class="calendar-text">节假日日历</text>
        <text class="calendar-desc">法定节假日/调休安排/多国日历</text>
      </view>
    </view>
  </view>
</view>
